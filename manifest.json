{
  "manifest_version": 3,

  "name": "Second Life Ticket Browser",
  "description":
    "This extension wraps API calls to JIRA for gathering status and metrics",
  "version": "1.1",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "images/favicon.png",
    "default_popup": "main.html",
    "default_title": "Metrics Activity for JIRA"
  },
  "permissions": ["activeTab", "https://jira.secondlife.com/", "storage"]
}
